@{
    string JSver = System.Configuration.ConfigurationManager.AppSettings["JSVersion"].ToString();
    string CSSver = System.Configuration.ConfigurationManager.AppSettings["CSSVersion"].ToString();
    string GJSver = System.Configuration.ConfigurationManager.AppSettings["GJSVersion"].ToString();
}


<!DOCTYPE html>
<html ng-app="app">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title  Home</title>

    @*/Css files/*@
    <link href="~/Content/assets/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/assets/fontawesome/css/all.min.css" rel="stylesheet" />
    <link href="~/Content/assets/css/sb-admin.css" rel="stylesheet" />
    @*<link href="~/Plugins/DataTable/css/datatable.min.css" rel="stylesheet" />*@
    <link href="~/Content/assets/css/style.css" rel="stylesheet" />
    <link href="~/Content/assets/css/dataTables.bootstrap4.css" rel="stylesheet" />
    <link href="~/Content/assets/css/Alertify/alertify.css" rel="stylesheet" />
    <link href="~/Content/assets/css/jquery.ui.css" rel="stylesheet" />

    @*/Js files/*@
    @*<script src="~/Content/assets/js/jquery.min.js"></script>*@
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <script src="~/Content/assets/js/dataTables/jquery.dataTables.js"></script>
    <script src="~/Content/assets/js/dataTables/dataTables.bootstrap4.js"></script>
    <script src="~/Content/assets/js/dataTables/datatables-demo.js"></script>
    <script src="~/Content/assets/js/bootstrap.bundle.min.js"></script>
    <script src="~/Content/assets/js/sb-admin.js"></script>
    <script src="~/Content/Angular/App/angular.js"></script>
    <script src="~/Plugins/DataTable/js/angular-datatables.js"></script>
    @*<script src="~/Scripts/angular-loader.js"></script>*@
    <script src="~/Content/assets/js/angular-loader.js"></script>
    <script src="~/Content/Angular/App/App.js"></script>
    @*<script src="~/Scripts/alertify.js"></script>
        <script src="~/Scripts/alertify.min.js"></script>*@
    <script src="~/Content/assets/js/alertify.js"></script>
    <script src="~/Content/assets/js/alertify.min.js"></script>
    <script src="~/Content/assets/js/jquery.alphanum.js"></script>
    <script src="~/Content/assets/js/jquery.numeric.js"></script>
    <script src="~/Content/assets/js/TextValidation.js"></script>
    <script src="~/Content/assets/js/jquery.ui.min.js"></script>
    <script src="~/Content/assets/js/Filesever.js"></script>
    <script src="~/Content/assets/js/Moment.min.js"></script>
    <script src="~/Content/assets/js/xlsx.core.min.js"></script>
    <script src="~/Content/assets/js/Alasql.min.js"></script>
    <script src="~/Scripts/Home/LayoutController.js"></script>
    <style type="text/css">
        #loadingDiv {
            position: fixed;
            top: 0px;
            right: 0px;
            width: 100%;
            height: 100%;
            background-color: #666;
            background-repeat: no-repeat;
            background-position: center;
            z-index: 10000000;
            opacity: 0.4;
            filter: alpha(opacity=40);
            text-align: center;
        }

            #loadingDiv .loading-spiner img {
                position: absolute;
                top: 50%;
                left: 50%;
                transform: translate(-50%, -50%);
                width: 150px;
            }
    </style>
</head>
<body>
    <div ng-controller="LayoutController">
        <nav class="navbar navbar-expand navbar-dark bg-dark static-top admin-header">
            <button class="btn btn-link btn-sm text-white order-1 order-sm-0" id="sidebarToggle" href="#">
                <i class="fas fa-bars"></i>
            </button>
            <a class="navbar-brand ml-3 header-logo" href="index.html">
                <img src="~/Content/assets/images/NSDC.png" />
            </a>

            <ul class="navbar-nav ml-auto ml-md-6">
                <li class="nav-item dropdown no-arrow">
                    <a class="nav-link dropdown-toggle" href="#" id="userDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        <span>Welcome, {{agencylist.Name}}</span>
                        <i class="fas fa-user-circle fa-fw"></i>
                    </a>
                    <div class="dropdown-menu dropdown-menu-right" aria-labelledby="userDropdown">
                        <a class="dropdown-item" href="#" id="btnResetPassword">Change Password</a>
                        <a class="dropdown-item" href="#" data-toggle="modal" data-target="#logoutModal">Logout</a>

                    </div>
                </li>
            </ul>
        </nav>
    </div>

    <div id="wrapper">
        <!-- Sidebar -->
        @Html.Partial("_Menu")
        <div class="container body-content">
            <!--Loader Section-->
            <div class="loading-spiner-holder" id="loadingDiv" data-loading>
                <div class="loading-spiner">
                    <img src="~/Scripts/Images/loader.gif" />
                </div>
            </div>
            <!--/Loader Section-->
            @RenderBody()
            <!-- Sticky Footer -->
            <footer class="sticky-footer">
                <div class="container my-auto">
                    <div class="copyright text-center my-auto">
                        <span>Copyright © @DateTime.Now.Year. All Rights Reserved.</span>
                    </div>
                </div>
            </footer>
        </div>
        <!-- /.content-wrapper -->
    </div>
    <!-- /#wrapper -->
    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>
    <!-- Logout Modal-->
    <div class="modal fade" id="logoutModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Ready to Leave?</h5>
                    <button class="close" type="button" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>
                </div>
                <div class="modal-body">Select "Logout" below if you are ready to end your current session.</div>
                <div class="modal-footer">
                    <button class="btn btn-secondary" type="button" data-dismiss="modal">Cancel</button>
                    <a class="btn btn-primary" href="#" id="btnLogout">Logout</a>
                </div>
            </div>
        </div>
    </div>


    <script src="~/Content/assets/js/Chart/fusioncharts.js"></script>
    <!-- Including the angularjs-fusioncharts directive-->
    <script src="~/Content/assets/js/Chart/angular.fusioncharts.js"></script>
    <!-- Including the fusion theme -->
    <script src="~/Content/assets/js/Chart/fusioncharts.theme.zune.js"></script>

    @*<link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css" />*@
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datetimepicker/4.17.37/css/bootstrap-datetimepicker.min.css" />


    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.10.6/moment.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datetimepicker/4.17.37/js/bootstrap-datetimepicker.min.js"></script>

    <script>
        (function ($) {
            "use strict"; // Start of use strict

            $(document).ready(function () {
                $('#dataTable').DataTable();
            });

            $("#sidebarToggle").on('click', function (e) {
                e.preventDefault();
                $("body").toggleClass("sidebar-toggled");
                $(".sidebar").toggleClass("toggled");
            });

            $("#btnLogout").click(function () {
                location.href = "/Home/Logout/";
                //= "/Home/Logout/";
            });
            $("#btnResetPassword").click(function () {
                location.href = "/Home/ChangePassword/";
                //= "/Home/Logout/";
            });
        })(jQuery); // End of use strict
    </script>
</body>
</html>